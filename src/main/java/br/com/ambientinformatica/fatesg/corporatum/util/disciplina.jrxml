	<queryString>
		<![CDATA[SELECT
     *,
     *,
     *,
     d."cargahoraria" AS d_cargahoraria,
     d."nome" AS d_nome,
     c."nome" AS c_nome,
     c."id" AS c_id,
     d."id" AS d_id,
     cd."id_colaborador" AS cd_id_colaborador,
     cd."id_disciplina" AS cd_id_disciplina
FROM
     "disciplina" d,
     "colaborador" c,
     "colaborador_disciplina" cd
WHERE
     $P{id_colaborador} = cd_id_colaborador
     and d_id = cd_id_disciplina]]>
	</queryString>
